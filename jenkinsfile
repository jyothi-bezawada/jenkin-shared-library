pipeline {
  agent {
    kubernetes { yaml jenkinsAgentpod.getpodwithcontainers(['nginx'])}
  }
   stages {
        stage ('build') {
            steps {
                sh 'python --version'
               }
             }
        stage ('build-nginx') {
          steps{
            conatainers ('nginx') {
               sh 'node --version'
            }
          }
        }
      }
}