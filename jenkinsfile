pipeline {
  agent {
    kubernetes { yaml jenkinsAgentpod.getpodwithcontainers(['nodejs14x'])}
  }
   stages {
        stage ('build') {
            steps {
                sh 'python --version'
               }
             }
        stage ('build-nodejs') {
          steps{
            conatainers ('nodejs') {
               sh 'node --version'
            }
          }
        }
      }
}
